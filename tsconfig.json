{
  "compilerOptions": {
    "target": "ESNext",
    "paths": {
      "@app/*": ["src/app/*"],
      "@data/*": ["src/data/*"],
    },
    "module": "commonjs",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true
  }
}
